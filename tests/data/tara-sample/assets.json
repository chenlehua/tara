{
  "project": {
    "name": "智能电动汽车TARA分析",
    "vehicle_type": "BEV",
    "vehicle_model": "EV-2025",
    "version": "1.0",
    "standard": "ISO/SAE 21434",
    "description": "针对智能电动汽车的威胁分析与风险评估"
  },
  "assets": [
    {
      "id": "ASSET-001",
      "name": "整车控制器 (VCU)",
      "type": "ECU",
      "category": "控制单元",
      "description": "负责整车动力系统协调控制，包括驱动电机、电池管理、能量回收等功能",
      "interfaces": [
        {"type": "CAN", "name": "动力CAN", "speed": "500Kbps"},
        {"type": "CAN", "name": "底盘CAN", "speed": "500Kbps"},
        {"type": "Ethernet", "name": "车载以太网", "speed": "100Mbps"}
      ],
      "security_properties": {
        "confidentiality": "high",
        "integrity": "critical",
        "availability": "critical"
      },
      "data_assets": [
        "车辆状态数据",
        "驱动控制指令",
        "故障诊断信息"
      ]
    },
    {
      "id": "ASSET-002",
      "name": "电池管理系统 (BMS)",
      "type": "ECU",
      "category": "控制单元",
      "description": "负责电池包的状态监控、充放电管理、热管理和安全保护",
      "interfaces": [
        {"type": "CAN", "name": "动力CAN", "speed": "500Kbps"},
        {"type": "LIN", "name": "从节点通信", "speed": "19.2Kbps"}
      ],
      "security_properties": {
        "confidentiality": "high",
        "integrity": "critical",
        "availability": "critical"
      },
      "data_assets": [
        "电池SOC/SOH",
        "单体电压温度",
        "充电状态"
      ]
    },
    {
      "id": "ASSET-003",
      "name": "电机控制器 (MCU)",
      "type": "ECU",
      "category": "控制单元",
      "description": "控制驱动电机的转速和扭矩输出",
      "interfaces": [
        {"type": "CAN", "name": "动力CAN", "speed": "500Kbps"}
      ],
      "security_properties": {
        "confidentiality": "medium",
        "integrity": "critical",
        "availability": "critical"
      },
      "data_assets": [
        "扭矩指令",
        "电机转速",
        "温度数据"
      ]
    },
    {
      "id": "ASSET-004",
      "name": "智能网关 (CGW)",
      "type": "Gateway",
      "category": "网络设备",
      "description": "负责车内各域网络之间的数据路由、协议转换和安全隔离",
      "interfaces": [
        {"type": "CAN", "name": "动力CAN", "speed": "500Kbps"},
        {"type": "CAN", "name": "底盘CAN", "speed": "500Kbps"},
        {"type": "CAN", "name": "车身CAN", "speed": "250Kbps"},
        {"type": "Ethernet", "name": "骨干网", "speed": "1Gbps"},
        {"type": "LIN", "name": "子网", "speed": "19.2Kbps"}
      ],
      "security_properties": {
        "confidentiality": "critical",
        "integrity": "critical",
        "availability": "critical"
      },
      "data_assets": [
        "路由表",
        "防火墙规则",
        "密钥材料"
      ]
    },
    {
      "id": "ASSET-005",
      "name": "远程通信单元 (T-Box)",
      "type": "Gateway",
      "category": "通信设备",
      "description": "提供车辆与云端的通信连接，支持远程控制、OTA升级等功能",
      "interfaces": [
        {"type": "4G/5G", "name": "蜂窝网络", "speed": "100Mbps"},
        {"type": "WiFi", "name": "车内热点", "speed": "300Mbps"},
        {"type": "Bluetooth", "name": "蓝牙", "speed": "3Mbps"},
        {"type": "CAN", "name": "车身CAN", "speed": "250Kbps"},
        {"type": "Ethernet", "name": "骨干网", "speed": "100Mbps"}
      ],
      "security_properties": {
        "confidentiality": "critical",
        "integrity": "critical",
        "availability": "high"
      },
      "data_assets": [
        "用户凭证",
        "OTA固件包",
        "远程指令",
        "位置数据"
      ]
    },
    {
      "id": "ASSET-006",
      "name": "智能驾驶控制器 (ADAS)",
