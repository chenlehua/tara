{
  "project": {
    "name": "智能网联汽车TARA分析样例",
    "vehicle_type": "新能源电动汽车",
    "standard": "ISO/SAE 21434"
  },
  "assets": [
    {
      "id": "ASSET-001",
      "name": "整车控制器 (VCU)",
      "type": "ECU",
      "description": "整车控制核心单元，负责整车动力、制动、转向等关键功能协调控制",
      "interfaces": [
        {"type": "CAN", "protocol": "CAN 2.0B", "baudrate": "500Kbps"},
        {"type": "Ethernet", "protocol": "SOME/IP", "bandwidth": "100Mbps"}
      ],
      "security_properties": {
        "confidentiality": "high",
        "integrity": "critical",
        "availability": "critical"
      },
      "functions": ["动力控制", "制动协调", "能量管理", "故障诊断"]
    },
    {
      "id": "ASSET-002",
      "name": "电池管理系统 (BMS)",
      "type": "ECU",
      "description": "电池组管理单元，负责电池状态监控、充放电管理、热管理",
      "interfaces": [
        {"type": "CAN", "protocol": "CAN 2.0B", "baudrate": "500Kbps"},
        {"type": "LIN", "protocol": "LIN 2.1", "baudrate": "19.2Kbps"}
      ],
      "security_properties": {
        "confidentiality": "high",
        "integrity": "critical",
        "availability": "critical"
      },
      "functions": ["SOC估算", "SOH管理", "充电控制", "热管理"]
    },
    {
      "id": "ASSET-003",
      "name": "智能网关 (CGW)",
      "type": "Gateway",
      "description": "车载中央网关，连接各个域控制器和外部网络",
      "interfaces": [
        {"type": "CAN", "protocol": "CAN FD", "baudrate": "2Mbps"},
        {"type": "Ethernet", "protocol": "DoIP", "bandwidth": "1Gbps"},
        {"type": "LIN", "protocol": "LIN 2.1", "baudrate": "19.2Kbps"}
      ],
      "security_properties": {
        "confidentiality": "critical",
        "integrity": "critical",
        "availability": "critical"
      },
      "functions": ["消息路由", "协议转换", "防火墙", "入侵检测"]
    },
    {
      "id": "ASSET-004",
      "name": "远程通信单元 (T-Box)",
      "type": "Telematics",
      "description": "远程信息处理单元，提供车辆远程控制和数据上传功能",
      "interfaces": [
        {"type": "4G/5G", "protocol": "LTE/NR", "bandwidth": "100Mbps"},
        {"type": "WiFi", "protocol": "802.11ac", "bandwidth": "300Mbps"},
        {"type": "Bluetooth", "protocol": "BLE 5.0", "bandwidth": "2Mbps"},
        {"type": "GNSS", "protocol": "GPS/Beidou", "bandwidth": "N/A"}
      ],
      "security_properties": {
        "confidentiality": "critical",
        "integrity": "critical",
        "availability": "high"
      },
      "functions": ["远程诊断", "OTA升级", "远程控制", "位置服务"]
    },
    {
      "id": "ASSET-005",
      "name": "信息娱乐系统 (IVI)",
      "type": "ECU",
      "description": "车载信息娱乐系统，提供导航、多媒体、车辆信息显示等功能",
      "interfaces": [
        {"type": "Ethernet", "protocol": "TCP/IP", "bandwidth": "100Mbps"},
        {"type": "USB", "protocol": "USB 3.0", "bandwidth": "5Gbps"},
        {"type": "Bluetooth", "protocol": "A2DP/HFP", "bandwidth": "3Mbps"}
      ],
      "security_properties": {
        "confidentiality": "medium",
        "integrity": "medium",
        "availability": "low"
      },
      "functions": ["导航", "多媒体", "语音助手", "手机互联"]
    },
    {
      "id": "ASSET-006",
      "name": "自动驾驶域控制器 (ADAS DCU)",
      "type": "Domain Controller",
      "description": "自动驾驶域控制器，处理感知融合和驾驶决策",
      "interfaces": [
        {"type": "Ethernet", "protocol": "AVB/TSN", "bandwidth": "10Gbps"},
        {"type": "CAN", "protocol": "CAN FD", "baudrate": "5Mbps"}
      ],
      "security_properties": {
        "confidentiality": "high",
        "integrity": "critical",
        "availability": "critical"
      },
      "functions": ["环境感知", "路径规划", "决策控制", "传感器融合"]
    },
    {
      "id": "ASSET-007",
      "name": "OBD诊断接口",
      "type": "Interface",
      "description": "车载诊断接口，用于故障诊断和维修",
      "interfaces": [
        {"type": "CAN", "protocol": "ISO 15765", "baudrate": "500Kbps"}
      ],
      "security_properties": {
        "confidentiality": "high",
        "integrity": "high",
        "availability": "medium"
      },
      "functions": ["故障读取", "参数配置", "软件刷写"]
    },
    {
      "id": "ASSET-008",
      "name": "充电通信控制器 (SECC)",
      "type": "ECU",
      "description": "充电通信控制器，负责与充电桩通信",
      "interfaces": [
        {"type": "PLC", "protocol": "HomePlug GP", "bandwidth": "10Mbps"},
        {"type": "CAN", "protocol": "CAN 2.0B", "baudrate": "250Kbps"}
      ],
      "security_properties": {
        "confidentiality": "high",
        "integrity": "critical",
        "availability": "high"
      },
      "functions": ["充电握手", "功率协商", "计费认证"]
    }
  ]
}
