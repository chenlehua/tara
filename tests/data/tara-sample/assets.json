      "security_properties": {
        "confidentiality": "high",
        "integrity": "critical",
        "availability": "critical"
      },
      "data_assets": [
        "感知数据",
        "决策数据",
        "控制指令"
      ]
    },
    {
      "id": "ASSET-007",
      "name": "前视摄像头",
      "type": "Sensor",
      "category": "传感器",
      "description": "提供前方道路图像数据用于车道保持和目标识别",
      "interfaces": [
        {"type": "Ethernet", "name": "视频流", "speed": "1Gbps"}
      ],
      "security_properties": {
        "confidentiality": "medium",
        "integrity": "high",
        "availability": "high"
      },
      "data_assets": [
        "视频流",
        "目标检测结果"
      ]
    },
    {
      "id": "ASSET-008",
      "name": "毫米波雷达",
      "type": "Sensor",
      "category": "传感器",
      "description": "提供前方障碍物距离和速度信息",
      "interfaces": [
        {"type": "CAN", "name": "感知CAN", "speed": "500Kbps"}
      ],
      "security_properties": {
        "confidentiality": "low",
        "integrity": "high",
        "availability": "high"
      },
      "data_assets": [
        "目标列表",
        "距离速度数据"
      ]
    },
    {
      "id": "ASSET-009",
      "name": "信息娱乐系统 (IVI)",
      "type": "ECU",
      "category": "信息娱乐",
      "description": "提供导航、多媒体、车辆设置等人机交互功能",
      "interfaces": [
        {"type": "Ethernet", "name": "骨干网", "speed": "100Mbps"},
        {"type": "CAN", "name": "车身CAN", "speed": "250Kbps"},
