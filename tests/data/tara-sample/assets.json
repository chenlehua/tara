  "network_topology": {
    "domains": [
      {
        "name": "动力域",
        "bus_type": "CAN",
        "assets": ["ASSET-001", "ASSET-002", "ASSET-003"]
      },
      {
        "name": "底盘域",
        "bus_type": "CAN",
        "assets": ["ASSET-006", "ASSET-008"]
      },
      {
        "name": "车身域",
        "bus_type": "CAN",
        "assets": ["ASSET-005", "ASSET-009"]
      },
      {
        "name": "ADAS域",
        "bus_type": "Ethernet",
        "assets": ["ASSET-006", "ASSET-007"]
      }
    ],
    "gateway": "ASSET-004"
  }
