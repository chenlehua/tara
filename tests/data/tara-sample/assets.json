      "data_assets": [
        "用户设置",
        "导航数据",
        "媒体文件"
      ]
    },
    {
      "id": "ASSET-010",
      "name": "OBD-II诊断接口",
      "type": "Interface",
      "category": "诊断接口",
      "description": "提供车辆诊断和标定功能的物理接口",
      "interfaces": [
        {"type": "CAN", "name": "诊断CAN", "speed": "500Kbps"}
      ],
      "security_properties": {
        "confidentiality": "high",
        "integrity": "critical",
        "availability": "low"
      },
      "data_assets": [
